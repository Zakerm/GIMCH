<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#C1B1BD"
    tools:context=".GenderActivity">

    <ImageButton
        android:id="@+id/buttonGenderLeft"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="472dp"
        android:background="@null"
        android:backgroundTint="#FCF5FD"
        android:focusable="auto"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/svgrepo_iconcarrier" />

    <Button
        android:id="@+id/buttonMan"
        android:layout_width="345dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="308dp"
        android:layout_marginEnd="29dp"
        android:backgroundTint="#FAEDED"
        android:fontFamily="@font/jura"
        android:shadowColor="#000000"
        android:text="Я парень"
        android:textColor="#000000"
        android:textSize="20dp"
        android:typeface="normal"
        app:cornerRadius="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#FF6F61"
        app:strokeColor="#FFBBB7"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/buttonGirl"
        android:layout_width="345dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="376dp"
        android:layout_marginEnd="29dp"
        android:backgroundTint="#FAEDED"
        android:fontFamily="@font/jura"
        android:shadowColor="#000000"
        android:text="Я девушка"
        android:textColor="#000000"
        android:textSize="20dp"
        android:typeface="normal"
        app:cornerRadius="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#FF6F61"
        app:strokeColor="#FFBBB7"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="31dp"
        android:backgroundTint="#C1B1BD"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#C1B1BD" />

    <Button
        android:id="@+id/button4"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="31dp"
        android:backgroundTint="#FFB3BA"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="."
        android:textSize="1sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#FFB3BA" />

    <Button
        android:id="@+id/button3"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="214dp"
        android:layout_marginTop="31dp"
        android:backgroundTint="#C1B1BD"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#C1B1BD" />

    <Button
        android:id="@+id/button6"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="311dp"
        android:layout_marginTop="31dp"
        android:backgroundTint="#C1B1BD"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#C1B1BD" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="410dp"
        android:layout_height="42dp"
        android:layout_marginTop="78dp"
        android:fontFamily="@font/jura_bold"
        android:text="Давай знакомиться!"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="291dp"
        android:layout_height="115dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="167dp"
        android:layout_marginEnd="50dp"
        android:fontFamily="@font/jura"
        android:lineSpacingExtra="0sp"
        android:text="Чтобы подобрать тебе лучших людей для общения, давай начнем с простого вопроса: кто ты?"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>