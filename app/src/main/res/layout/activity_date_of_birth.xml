<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DateOfBirthActivity">

    <ImageButton
        android:id="@+id/buttonDateOfBirthRight"
        android:layout_width="108dp"
        android:layout_height="73dp"
        android:layout_marginTop="640dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#FFFFFF"
        android:scaleType="centerInside"
        android:tint="#6D6767"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/right_chevron__1_" />

    <ImageButton
        android:id="@+id/buttonDateOfBirthLeft"
        android:layout_width="108dp"
        android:layout_height="73dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="640dp"
        android:backgroundTint="#FFFFFF"
        android:focusable="auto"
        android:rotationY="-180"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/right_chevron__1_" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="43dp"
        android:layout_height="31dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="272dp"
        android:fontFamily="@font/inknut_antiqua_semibold"
        android:lineSpacingExtra="-24dp"
        android:text="День"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="321dp"
        android:layout_height="107dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="136dp"
        android:fontFamily="@font/inknut_antiqua_semibold"
        android:lineSpacingExtra="-24dp"
        android:text="Важно ввести реальную дату — позже изменить её не получится."
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextDate3"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="304dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="12"
        android:inputType="date"
        android:paddingLeft="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="304dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="18"
        android:inputType="date"
        android:paddingLeft="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="57dp"
        android:layout_height="33dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="272dp"
        android:fontFamily="@font/inknut_antiqua_semibold"
        android:lineSpacingExtra="-24dp"
        android:text="Месяц"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="57dp"
        android:layout_height="33dp"
        android:layout_marginStart="260dp"
        android:layout_marginTop="272dp"
        android:fontFamily="@font/inknut_antiqua_semibold"
        android:lineSpacingExtra="-24dp"
        android:text="Год"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/inknut_antiqua_black"
        android:text="Укажи дату рождения"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="29dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button15"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#C92197"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="."
        android:textSize="1sp"
        app:layout_constraintStart_toEndOf="@+id/button13"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#C1B1BD"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:layout_constraintStart_toEndOf="@+id/button14"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button13"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="325dp"
        android:backgroundTint="#C92197"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="."
        android:textSize="1sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button14"
        android:layout_width="70dp"
        android:layout_height="6dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#C92197"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="."
        android:textSize="1sp"
        app:layout_constraintStart_toEndOf="@+id/button15"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="157dp"
        android:layout_height="60dp"
        android:layout_marginStart="208dp"
        android:layout_marginTop="304dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="2002"
        android:inputType="date"
        android:paddingLeft="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="206dp"
        android:layout_height="206dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="404dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/free_icon_16_10540760" />
</androidx.constraintlayout.widget.ConstraintLayout>